"local Material = loadstring(game:HttpGet(\"https://raw.githubusercontent.com/HairBaconGamming/Gamming-Material-HUB/main/Main.lua\"))()\r\n\r\nlocal Notes = workspace.Notes\r\n\r\nlocal k1 = Color3.fromRGB(15, 157, 88)\r\nlocal k2 = Color3.fromRGB(219, 68, 55)\r\nlocal k3 = Color3.fromRGB(66, 133, 244)\r\nlocal k4 = Color3.fromRGB(244, 160, 0)\r\n\r\nlocal notekeys = {\r\n\t[1] = Enum.KeyCode.A,\r\n\t[2] = Enum.KeyCode.S,\r\n\t[3] = Enum.KeyCode.K,\r\n\t[4] = Enum.KeyCode.L\r\n}\r\n\r\nlocal mytrack = workspace.ActiveTrack:GetChildren()[1]\r\n\r\nif _G.Working then\r\n\tlocal a = game.CoreGui:FindFirstChild(\"Gamming Hub\")\r\n\tif a then\r\n\t\ta:Destroy()\r\n\tend\r\n\tif _G.ConnectAuto then\r\n\t\t_G.ConnectAuto:Disconnect()\r\n\tend\r\n\tif _G.togglekey then\r\n\t\t_G.togglekey:Disconnect()\r\n\tend\r\nend\r\n\r\n_G.Working = true\r\n\r\nlocal Main = Material:Load({\r\n\tTitle = \"Gamming Hub\",\r\n\tSmothDrag = false,\r\n\tKey = \"RightControl\" or Enum.KeyCode.RightControl,\r\n})\r\n\r\nlocal Temp = Main:New({\r\n\tTitle = \"Home\",\r\n\tImageId = 4034483344,\r\n\tImage = \"http://www.roblox.com/asset/?id=4034483344\",\r\n})\r\n\r\nlocal currentkey = Enum.KeyCode.T\r\nlocal toggle = false\r\n\r\nlocal distance = .5\r\n\r\nfunction OnClick()\r\n\t\r\nend\r\n\r\nfunction OnChange(key:Enum.KeyCode)\r\n\tcurrentkey = key\r\nend\r\n\r\nlocal Toggle = Temp:Toggle({\r\n\tTitle = \"Auto\",\r\n\tCallback = OnClick, -- return nil\r\n\tEnabled = toggle\r\n})\r\n\r\nlocal KeyBox = Temp:KeyBox({\r\n\tTitle = \"Key Toggle\",\r\n\tCallback = OnChange, -- return \u003Ckeycode>\r\n\tKey = currentkey,\r\n})\r\n\r\nlocal Slide = Temp:Slide({\r\n\tTitle = \"DistanceCanPress\",\r\n\tCallback = function(Value)\r\n\t\tdistance = Value\r\n\tend,\r\n\tMin = 0,\r\n\tMax = 2,\r\n\tDefault = distance,\r\n\tPercent = 100, -- example: 23 >> 23.2\r\n})\r\n\r\n_G.togglekey = game:GetService(\"UserInputService\").InputBegan:Connect(function(Input)\r\n\tif Input.KeyCode == currentkey then\r\n\t\ttoggle = not toggle\r\n\t\tToggle:SetToggle(toggle)\r\n\tend\r\nend)\r\n\r\nfunction play(note,ktype)\r\n\tlocal endp = mytrack:FindFirstChild(\"End\"..ktype)\r\n\tif endp then\r\n\t\tlocal mag = (note.Position-endp.Position).Magnitude\r\n\t\tif mag \u003C= distance then\r\n\t\t\tkeypress(notekeys[ktype])\r\n\t\t\tnote.Transparency = 1\r\n\t\tend\r\n\tend\r\nend\r\n\r\n_G.ConnectAuto = game:GetService(\"RunService\").Heartbeat:Connect(function()\r\n\tif toggle then\r\n\t\tfor i,v:BasePart in pairs(Notes:GetChildren()) do\r\n\t\t\tif v.Transparency == 1 then continue end\r\n\t\t\tif v.Color == k1 then\r\n\t\t\t\tplay(v,1)\r\n\t\t\telseif v.Color == k2 then\r\n\t\t\t\tplay(v,2)\r\n\t\t\telseif v.Color == k3 then\r\n\t\t\t\tplay(v,3)\r\n\t\t\telseif v.Color == k4 then\r\n\t\t\t\tplay(v,4)\r\n\t\t\tend\r\n\t\tend\r\n\tend\r\nend)"